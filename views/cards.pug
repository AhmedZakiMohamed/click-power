    background: linear-gradient(135deg, #1d4ed8 0%, #0284c7 100%);
    transform: translateY(-2px) scale(1.03);
    box-shadow: 0 6px 20px rgba(37, 99, 235, 0.4);
    }
// Navbar
nav#navbar.navbar
  .nav-container
    //- Logo
    a.logo(href='/')
      img.logo-icon(src='/img/logo.png' alt='Click Power Logo')
      span Click Power
    
    //- Menu Toggle للموبايل
    #menuToggle.menu-toggle
      span
      span
      span
    
    //- Nav Links
    ul#navLinks.nav-links
      li
        a(href='/') 🏠 Home
      li
        a(href='/about') ℹ️ About Us
      li
        a(href='/cards') 🛒 Products
      li
        a(href='/contact') ✉️ Contact
      
